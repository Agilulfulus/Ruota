load "System";

Fibonacci(x) => a from {
	a = [0, 1];
	i in [3 : x] do {
		a push a[i - 1] + a[i - 2];
	};
};

len args switch {
0 >> os.printl(Fibonacci(10));
1 >> os.printl(Fibonacci(num args[1]));
} >> os.printl("Improper Arguments");

